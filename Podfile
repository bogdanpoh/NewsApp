# Uncomment the next line to define a global platform for your project
 platform :ios, '13.0'

target 'NewsApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # UI
  pod 'SnapKit'

  # Arhitecture
  pod 'Swinject'
  
  # Rx
  pod 'RxSwift'
  pod 'RxCocoa'
  
  # Async
  pod "PromiseKit"
  
  # Resources
  pod 'R.swift'
  pod 'Kingfisher'

end
